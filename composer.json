{
    "name":              "ocramius/change-set",
    "description":       "A library to compute, commit and rollback simple changesets, tracking insertions, deletions and updates",
    "type":              "library",
    "license":           "MIT",
    "homepage":          "https://github.com/Ocramius/ChangeSet",
    "minimum-stability": "dev",
    "keywords":          [
        "unit of work",
        "changeset",
        "diff",
        "change set"
    ],
    "authors": [
        {
            "name":     "Marco Pivetta",
            "email":    "ocramius@gmail.com",
            "homepage": "http://ocramius.github.com/"
        }
    ],
    "require": {
        "php": "~5.5 || ^7.1",
        "zendframework/zend-eventmanager": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit":           "~4.0",
        "squizlabs/php_codesniffer": "~2.0@RC",
        "zendframework/zend-stdlib": "~2.3",
        "pimple/pimple":             "1.0.*",
        "behat/behat":               "~3.0"
    },
    "autoload": {
        "psr-4": {
            "ChangeSet\\": "src/ChangeSet/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "ChangeSetTest\\":        "test",
            "ChangeSetTestAsset\\":   "test",
            "ChangeSetTestContext\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
